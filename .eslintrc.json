{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:cypress/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-param-reassign": 0,
      "import/extensions": [0, { "js": "always" }]
    }
}
